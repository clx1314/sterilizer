/* pages/history/history.wxss */
.records{
    padding: 0 20px;
    padding-bottom: 50px;
}
.record{
    background: #fff;
    width: 100%;
    /* height: 200rpx; */
    border-radius: 18rpx;
    box-shadow: 4rpx 6rpx 10rpx #ddd;
    padding: 16rpx 30rpx;
    box-sizing: border-box;
    margin-top: 30rpx;
}
.flex_seat{
    display: flex;
    justify-content: space-between;
    align-items: center;
    line-height: 80rpx;
}
.iconName{
    width: 170rpx;
}
.time{
    color: #666;
    font-size: 28rpx;
}
.time_temp{
    width: 240rpx;
    font-size: 24rpx;
    color: #999;
}